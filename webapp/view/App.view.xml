<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="de.integrata.training.controller.App"
	xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>pgTitle}">
				<content>
					<List
						headerText="{i18n>titleFlightCustomer}"
						items="{model: 'flugkundenModel', path: '/d/results'}" >
						<items>
							<StandardListItem title="{flugkundenModel>Custname}" description="{flugkundenModel>Street}, {flugkundenModel>City}"/>
						</items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>